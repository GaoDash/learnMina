<import src='/templates/movie-template/stars-template/stars-template.wxml' />
<template name='movieTemplate'>
  <view class='movie-container'>
    <image class='movie-banner' src='{{coverUrl}}'></image>
    <text class='movie-name'>{{title}}</text>
    <template is='starsTemplate' data='{{average, stars}}' />
  </view>
</template>